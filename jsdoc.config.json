{
    "source": {
        "include": ["module.js", "README.md"],
        "exclude": ["./node_modules"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
        "destination": "./docs",
        "recurse": true,
        "readme": "./README.md"
    }
}
